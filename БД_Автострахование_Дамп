-- Таблица: СтраховыеСлучаи
CREATE TABLE `СтраховыеСлучаи` (
  `КодСлучая` INT NOT NULL,
  `Описание` VARCHAR(500) NULL,
  `КодТипа` INT NOT NULL,
  PRIMARY KEY (`КодСлучая`),
  CONSTRAINT `fk_СтраховыеСлучаи_ТипыАвто1`
    FOREIGN KEY (`КодТипа`)
    REFERENCES `ТипыАвто` (`КодТипа`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);
