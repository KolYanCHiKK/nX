-- Таблица: Сотрудники
CREATE TABLE `Сотрудники` (
  `КодСотрудника` INT NOT NULL,
  `Фамилия` VARCHAR(45) NULL,
  `Имя` VARCHAR(45) NULL,
  `Отчество` VARCHAR(45) NULL,
  `Адрес` VARCHAR(45) NULL,
  `Должность` VARCHAR(60) NULL,
  `Зарплата` MEDIUMINT NULL,
  `ДатаЗачисления` DATE NULL,
  `КодОтдела` INT NOT NULL,
  PRIMARY KEY (`КодСотрудника`),
  CONSTRAINT `fk_Сотрудники_Отделы1`
    FOREIGN KEY (`КодОтдела`)
    REFERENCES `Отделы` (`КодОтдела`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);
INSERT INTO Сотрудники (КодСотрудника, Фамилия, Имя, Отчество, Адрес, Должность, Зарплата, ДатаЗачисления, КодОтдела) VALUES (1, 'Смирнов', 'Артем', 'Владимирович', 'Москва', 'Менеджер', 60000, '2022-03-15', 1);
