-- Таблица: Выплаты
CREATE TABLE `Выплаты` (
  `КодВыплаты` INT NOT NULL,
  `Сумма` INT NULL,
  `НомерСчета` VARCHAR(40) NULL,
  `БИК` VARCHAR(45) NULL,
  `КБК` VARCHAR(45) NULL,
  `БанкПолучателя` VARCHAR(100) NULL,
  `КодОбращения` INT NOT NULL,
  PRIMARY KEY (`КодВыплаты`),
  CONSTRAINT `fk_Выплаты_Обращения1`
    FOREIGN KEY (`КодОбращения`)
    REFERENCES `Обращения` (`КодОбращения`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);
INSERT INTO Выплаты (КодВыплаты, Сумма, НомерСчета, БИК, КБК, БанкПолучателя, КодОбращения) VALUES (1, 100000, '40817810099910000001', '044525225', '12345678901234567890', 'Сбербанк', 1);
