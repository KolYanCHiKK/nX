PRAGMA foreign_keys = off;
BEGIN TRANSACTION;

-- Таблица: Автомобили
CREATE TABLE `Автомобили` (
  `КодАвто` INT NOT NULL,
  `Наименование` VARCHAR(80) NULL,
  `ГодВыпуска` SMALLINT NULL,
  `Пробег` INT NULL,
  `Состояние` VARCHAR(45) NULL,
  `КодТипа` INT NOT NULL,
  `СТС` VARCHAR(30) NULL,
  `ПТС` VARCHAR(30) NULL,
  `Номер` VARCHAR(45) NULL,
  PRIMARY KEY (`КодАвто`),
  CONSTRAINT `fk_Автомобили_ТипыАвто1`
    FOREIGN KEY (`КодТипа`)
    REFERENCES `ТипыАвто` (`КодТипа`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);
