-- Таблица: ВидыСтрахования
CREATE TABLE `ВидыСтрахования` (
  `КодВида` INT NOT NULL,
  `Наименование` VARCHAR(45) NULL,
  `ТипКлиента` VARCHAR(45) NULL,
  `МаксСумма` INT NULL,
  PRIMARY KEY (`КодВида`));
