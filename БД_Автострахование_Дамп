-- Таблица: Договоры
CREATE TABLE `Договоры` (
  `КодДоговора` INT NOT NULL,
  `КодАвто` INT NOT NULL,
  `Дата` DATE NULL,
  `КодВида` INT NOT NULL,
  `СуммаПремии` MEDIUMINT NULL,
  PRIMARY KEY (`КодДоговора`),
  CONSTRAINT `fk_Клиенты_has_Автомобили_Автомобил1`
    FOREIGN KEY (`КодАвто`)
    REFERENCES `Автомобили` (`КодАвто`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_Договоры_ВидыСтрахования1`
    FOREIGN KEY (`КодВида`)
    REFERENCES `ВидыСтрахования` (`КодВида`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);
INSERT INTO Договоры (КодДоговора, КодАвто, Дата, КодВида, СуммаПремии) VALUES (1, 1, '2024-01-10', 1, 100000);
INSERT INTO Договоры (КодДоговора, КодАвто, Дата, КодВида, СуммаПремии) VALUES (2, 2, '2024-01-15', 2, 15000);
INSERT INTO Договоры (КодДоговора, КодАвто, Дата, КодВида, СуммаПремии) VALUES (3, 3, '2024-01-20', 3, 50000);
INSERT INTO Договоры (КодДоговора, КодАвто, Дата, КодВида, СуммаПремии) VALUES (4, 4, '2024-01-25', 4, 200000);
INSERT INTO Договоры (КодДоговора, КодАвто, Дата, КодВида, СуммаПремии) VALUES (5, 5, '2024-02-01', 5, 75000);

-- Таблица: Договоры_по_Клиенты
CREATE TABLE `Договоры_по_Клиенты` (
  `КодКлиента` INT NOT NULL,
  `КодДоговора` INT NOT NULL,
  `Класс` SMALLINT NULL,
  PRIMARY KEY (`КодКлиента`, `КодДоговора`),
  CONSTRAINT `fk_Клиенты_has_Договоры_Клиенты1`
    FOREIGN KEY (`КодКлиента`)
    REFERENCES `Клиенты` (`КодКлиента`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_Клиенты_has_Договоры_Договоры1`
    FOREIGN KEY (`КодДоговора`)
    REFERENCES `Договоры` (`КодДоговора`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);
INSERT INTO Договоры_по_Клиенты (КодКлиента, КодДоговора, Класс) VALUES (1, 1, 1);
INSERT INTO Договоры_по_Клиенты (КодКлиента, КодДоговора, Класс) VALUES (2, 2, 2);
INSERT INTO Договоры_по_Клиенты (КодКлиента, КодДоговора, Класс) VALUES (3, 3, 3);
INSERT INTO Договоры_по_Клиенты (КодКлиента, КодДоговора, Класс) VALUES (4, 4, 4);
INSERT INTO Договоры_по_Клиенты (КодКлиента, КодДоговора, Класс) VALUES (5, 5, 5);

