-- Таблица: Сотрудники_по_Договоры
CREATE TABLE `Сотрудники_по_Договоры` (
  `КодСотрудника` INT NOT NULL,
  `КодДоговора` INT NOT NULL,
  PRIMARY KEY (`КодСотрудника`, `КодДоговора`),
  CONSTRAINT `fk_Сотрудники_has_Договоры_Сотрудни1`
    FOREIGN KEY (`КодСотрудника`)
    REFERENCES `Сотрудники` (`КодСотрудника`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_Сотрудники_has_Договоры_Договоры1`
    FOREIGN KEY (`КодДоговора`)
    REFERENCES `Договоры` (`КодДоговора`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);
