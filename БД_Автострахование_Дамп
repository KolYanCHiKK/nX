-- Таблица: Обращения
CREATE TABLE `Обращения` (
  `КодОбращения` INT NOT NULL,
  `НомерФиксации` INT NULL,
  `Дата` DATE NULL,
  `КодКлиента` INT NOT NULL,
  `КодДоговора` INT NOT NULL,
  `СтатусОбращения` VARCHAR(45) NULL,
  PRIMARY KEY (`КодОбращения`),
  CONSTRAINT `fk_Обращения_Клиенты1`
    FOREIGN KEY (`КодКлиента`)
    REFERENCES `Клиенты` (`КодКлиента`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_Обращения_Договоры1`
    FOREIGN KEY (`КодДоговора`)
    REFERENCES `Договоры` (`КодДоговора`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);
INSERT INTO Обращения (КодОбращения, НомерФиксации, Дата, КодКлиента, КодДоговора, СтатусОбращения) VALUES (1, 1001, '2024-02-05', 1, 1, 'Рассмотрено');
